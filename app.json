{
  "expo": {
    "name": "Trivia Challenge",
    "slug": "trivia-trundle",
    "description": "A quick true or false game.",
    "privacy": "unlisted",
    "githubUrl": "https://github.com/louiechristie/trivia-trundle",
    "version": "2.0.1",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "trivia-trundle",
    "userInterfaceStyle": "automatic",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "debug": false
    }
  }
}
