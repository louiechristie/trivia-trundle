{
  "expo": {
    "name": "Trivia Trundle",
    "slug": "trivia-trundle",
    "description": "A quick true or false game.",
    "privacy": "unlisted",
    "githubUrl": "https://github.com/louiechristie/trivia-trundle",
    "version": "3.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "trivia-trundle",
    "userInterfaceStyle": "automatic",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/99f63609-a66b-4014-af5d-ea4a7efa7a93"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.louiechristie.triviatrundle",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "debug": false,
      "eas": {
        "projectId": "99f63609-a66b-4014-af5d-ea4a7efa7a93"
      }
    },
    "experiments": {
      "baseUrl": "/trivia-trundle"
    },
    "android": {
      "package": "com.louiechristie.triviatrundle"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
