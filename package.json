{
  "author": "Louie Christie <6807448+louiechristie@users.noreply.github.com> (https://github.com/louiechristie/)",
  "name": "trivia-trundle",
  "description": "A a 10 question, true or false, trivia game",
  "version": "2.2.0",
  "private": true,
  "license": "LicenseRef-LICENSE",
  "main": "node_modules/expo/AppEntry.js",
  "homepage": "/trivia-trundle/",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll",
    "deploy": "yarn deploy:expo && yarn build:web && yarn deploy:web",
    "deploy:expo": "npx expo publish",
    "build:web": "npx expo build:web",
    "deploy:web": "yarn build:web && rsync -avP ~/Documents/trivia-trundle/web-build/ pi@192.168.178.29:/var/www/html/trivia-trundle",
    "lint": "yarn lint:ts && yarn check:ts && yarn lint:md",
    "check:ts": "tsc --noEmit",
    "lint:ts": "eslint '**/*.+(js|jsx|ts|tsx)' --ignore-path .gitignore",
    "lint:md": "markdownlint '**/README.md' --ignore node_modules"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && yarn lint-staged"
    }
  },
  "lint-staged": {
    "**/*.+(md)": [
      "prettier --write",
      "markdownlint --ignore node_modules --fix"
    ],
    "**/*.+(js|jsx|ts|tsx|css|sass|less|graphql|yml|yaml|scss|vue)": [
      "prettier --write",
      "eslint --ignore-path .gitignore --fix",
      "jest --findRelatedTests"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/material-bottom-tabs": "^6.2.16",
    "@react-navigation/material-top-tabs": "^6.6.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "expo": "^49.0.11",
    "expo-asset": "~8.10.1",
    "expo-constants": "~14.4.2",
    "expo-font": "~11.4.0",
    "expo-linking": "~5.0.2",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-updates": "~0.18.13",
    "expo-web-browser": "~12.3.2",
    "html-entities": "^2.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.4",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-paper": "^4.12.5",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.5",
    "@types/react": "~18.2.14",
    "@types/react-native": "~0.72.2",
    "babel-preset-expo": "^9.5.0",
    "eslint": "^8.49.0",
    "eslint-config-universe": "^11.3.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "expo-cli": "^6.3.12",
    "husky": "^8.0.3",
    "jest-expo": "^49.0.0",
    "lint-staged": "^13.3.0",
    "markdownlint-cli": "^0.36.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  }
}
