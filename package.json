{
  "author": "Louie Christie <6807448+louiechristie@users.noreply.github.com> (https://github.com/louiechristie/)",
  "name": "trivia-trundle",
  "description": "A a 10 question, true or false, trivia app",
  "version": "0.1.0",
  "private": true,
  "license": "LicenseRef-LICENSE",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll",
    "deploy": "yarn deploy:expo && yarn build:web && yarn deploy:web",
    "deploy:expo": "expo publish",
    "build:web": "expo build:web",
    "deploy:web": "netlify deploy --prod",
    "lint": "yarn lint:ts && yarn lint:md",
    "lint:ts": "eslint '**/*.+(js|jsx|ts|tsx)' --ignore-path .gitignore",
    "lint:md": "markdownlint '**/*.+(md)' --ignore node_modules"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "**/*.+(md)": [
      "prettier --write",
      "markdownlint --ignore node_modules --fix"
    ],
    "**/*.+(js|jsx|ts|tsx|css|sass|less|graphql|yml|yaml|scss|vue)": [
      "prettier --write",
      "eslint --ignore-path .gitignore --fix",
      "jest --findRelatedTests"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "^5.6.1",
    "@react-navigation/native": "^5.6.1",
    "@react-navigation/stack": "^5.6.2",
    "expo": "^38.0.0",
    "expo-asset": "~8.1.7",
    "expo-constants": "~9.1.1",
    "expo-font": "~8.2.1",
    "expo-linking": "^1.0.1",
    "expo-splash-screen": "^0.3.1",
    "expo-status-bar": "^1.0.0",
    "expo-web-browser": "~8.3.1",
    "html-entities": "^1.3.1",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.2.tar.gz",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-safe-area-context": "~3.0.7",
    "react-native-screens": "~2.9.0",
    "react-native-web": "~0.11.7"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@types/react": "~16.9.41",
    "@types/react-native": "~0.62.13",
    "babel-preset-expo": "^8.2.3",
    "eslint": "^7.4.0",
    "eslint-config-universe": "^4.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react-hooks": "^4.0.6",
    "eslint-plugin-tsdoc": "^0.2.5",
    "expo-cli": "^3.22.3",
    "husky": "^4.2.5",
    "jest-expo": "^38.0.0",
    "lint-staged": "^10.2.11",
    "markdownlint-cli": "^0.23.2",
    "prettier": "^2.0.5",
    "typescript": "~3.9.5"
  }
}
