{
  "author": "Louie Christie <6807448+louiechristie@users.noreply.github.com> (https://github.com/louiechristie/)",
  "name": "trivia-trundle",
  "description": "A a 10 question, true or false, trivia game",
  "version": "1.0.3",
  "private": true,
  "license": "LicenseRef-LICENSE",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll",
    "deploy": "yarn deploy:expo && yarn build:web && yarn deploy:web",
    "deploy:expo": "expo publish",
    "build:web": "expo build:web",
    "deploy:web": "netlify deploy --prod",
    "lint": "yarn lint:ts && yarn check:ts && yarn lint:md",
    "check:ts": "tsc --noEmit",
    "lint:ts": "eslint '**/*.+(js|jsx|ts|tsx)' --ignore-path .gitignore",
    "lint:md": "markdownlint '**/README.md' --ignore node_modules"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && yarn lint-staged"
    }
  },
  "lint-staged": {
    "**/*.+(md)": [
      "prettier --write",
      "markdownlint --ignore node_modules --fix"
    ],
    "**/*.+(js|jsx|ts|tsx|css|sass|less|graphql|yml|yaml|scss|vue)": [
      "prettier --write",
      "eslint --ignore-path .gitignore --fix",
      "jest --findRelatedTests"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "^5.11.10",
    "@react-navigation/material-bottom-tabs": "^5.3.15",
    "@react-navigation/material-top-tabs": "^5.3.15",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.4",
    "expo": "^45.0.0-beta.9",
    "expo-asset": "~8.5.0",
    "expo-constants": "~13.1.0",
    "expo-font": "~10.1.0",
    "expo-linking": "~3.1.0",
    "expo-splash-screen": "~0.15.1",
    "expo-status-bar": "~1.3.0",
    "expo-web-browser": "~10.2.0",
    "html-entities": "^2.3.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.1",
    "react-native-gesture-handler": "~2.2.0",
    "react-native-paper": "^4.8.1",
    "react-native-reanimated": "~2.8.0",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "~3.11.1",
    "react-native-tab-view": "^3.0.1",
    "react-native-web": "0.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/jest": "^26.0.23",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.67.6",
    "babel-preset-expo": "~9.1.0",
    "eslint": "^7.25.0",
    "eslint-config-universe": "^7.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "expo-cli": "^4.4.3",
    "husky": "^6.0.0",
    "jest-expo": "^45.0.0-beta",
    "lint-staged": "^10.5.4",
    "markdownlint-cli": "^0.27.1",
    "prettier": "^2.2.1",
    "typescript": "~4.3.5"
  }
}
